    "compile": "rm -rf dist && babel --presets es2015 -d dist/ src/",
    "prepublish": "npm run compile"
  },
  "engines": {
    "node": ">=4.3.2"
  },
  "engine-strict": true,
  "keywords": [
    "nodejs",
    "congress",
