{
  "name": "house-gov-page-scraper",
  "version": "0.1.0",
  "description": "a node module for scraping info from the US House of Representatives website: house.gov",
  "main": "./dist/index.js",
  "files": [
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/chrisdevwords/house-gov-page-scraper"
  },
  "scripts": {
    "test": "nyc mocha ./test",
